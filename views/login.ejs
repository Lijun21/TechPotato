                    <label>Password: </label>
                    <input class="form-control" type="password" name="password" placeholder="5+ characters">
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">login </button>
                </div>
            </form>
                <a href="/projects">Go Back</a>
                <a href="/forgot" class="pull-right" id="forgot">Forgot your password? </a>
        </div>
    </div>
</div>
